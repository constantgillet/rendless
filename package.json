{
  "name": "rendless-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prepare": "panda codegen",
    "build": "paraglide-js compile --project ./project.inlang --outdir ./app/paraglide && prisma generate && prisma migrate deploy && remix vite:build",
    "dev": "tsx ./server.ts",
    "biome-fix": "npx @biomejs/biome lint --apply ./app",
    "start": "cross-env NODE_ENV=production tsx ./server.ts",
    "typecheck": "tsc",
    "postinstall": "paraglide-js compile --project ./project.inlang --outdir ./app/paraglide",
    "react-email": "email dev --port 3001"
  },
