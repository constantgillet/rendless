{
  "name": "ogimage",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prepare": "panda codegen",
    "build": "npm run build:remix && npm run build:server",
    "build:remix": "remix build",
    "build:server": "esbuild --format=esm --platform=node --bundle ./server.ts --outfile=build/server.js --packages=external",
    "dev": "concurrently --names SERVER,REMIX --prefix-colors bgGreen.gray.bold,bgBlue.gray.bold \"npm run dev:build\" \"npm run dev:remix\"",
    "dev:build": "cross-env NODE_ENV=production npm run build:server -- --watch",
    "dev:remix": "remix dev --manual -c \"node --watch-path ./build/server.js --watch ./build/server.js\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@lucia-auth/adapter-prisma": "^4.0.0",
    "@prisma/client": "^5.9.1",
    "@radix-ui/themes": "^2.0.2",
    "@redux-devtools/extension": "^3.3.0",
    "@remix-run/css-bundle": "^2.4.0",
    "@remix-run/express": "^2.6.0",
    "@remix-run/node": "^2.4.0",
    "@remix-run/react": "^2.4.0",
    "@remix-validated-form/with-zod": "^2.0.7",
    "cookie-parser": "^1.4.6",
    "isbot": "^3.6.8",
    "lucia": "^3.0.1",
    "oslo": "^1.0.4",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.4.1",
    "react-moveable": "^0.56.0",
    "react-virtuoso": "^4.6.2",
    "remix-validated-form": "^5.1.5",
    "satori": "^0.10.13",
    "usehooks-ts": "^2.14.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.22.0",
    "@remix-run/dev": "^2.4.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.20",
    "@types/react-color": "^3.0.11",
    "@types/react-dom": "^18.2.7",
    "@types/source-map-support": "^0.5.10",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prisma": "^5.9.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
